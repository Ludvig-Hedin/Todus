{
  "name": "@zero/native",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "macos": "react-native run-macos",
    "ios:build:compile": "xcodebuild -quiet -workspace ios/ZeroNative.xcworkspace -scheme ZeroNative -configuration Debug -sdk iphonesimulator -destination 'generic/platform=iOS Simulator' -derivedDataPath ios/build/DerivedData build CODE_SIGNING_ALLOWED=NO",
    "macos:build:compile": "xcodebuild -quiet -workspace macos/zero-native.xcworkspace -scheme zero-native-macOS -configuration Debug -derivedDataPath macos/build/DerivedData build CODE_SIGNING_ALLOWED=NO",
    "pod-install": "bundle exec pod install --project-directory=ios && bundle exec pod install --project-directory=macos",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint .",
    "start": "react-native start",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "5.2.8",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "7.14.0",
    "@react-navigation/drawer": "7.8.1",
    "@react-navigation/native": "^7.1.19",
    "@react-navigation/native-stack": "^7.3.29",
    "@shopify/flash-list": "2.2.2",
    "@tanstack/query-async-storage-persister": "^5.90.2",
    "@tanstack/react-query": "^5.90.2",
    "@trpc/server": "catalog:",
    "@trpc/tanstack-react-query": "catalog:",
    "@zero/api-client": "workspace:*",
    "@zero/design-tokens": "workspace:*",
    "@zero/shared": "workspace:*",
    "@zero/ui-native": "workspace:*",
    "jotai": "2.13.1",
    "lucide-react-native": "0.575.0",
    "react": "19.1.4",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-macos": "0.81.2",
    "react-native-reanimated": "4.2.2",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.17.1",
    "react-native-svg": "15.15.3",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.81.5",
    "@react-native/eslint-config": "0.81.5",
    "@react-native/metro-config": "0.81.5",
    "@react-native/typescript-config": "0.81.5",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.4",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.4",
    "typescript": "catalog:"
  },
  "engines": {
    "node": ">=20"
  }
}
